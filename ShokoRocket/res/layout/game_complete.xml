<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_height="wrap_content" android:orientation="vertical" android:layout_width="fill_parent">
  	<ImageView android:id="@+id/ImageView01" android:layout_width="wrap_content" android:layout_height="wrap_content" android:background="@drawable/complete" android:layout_gravity="center_horizontal" android:layout_margin="3dip"></ImageView>
	<TextView android:id="@+id/TextView01" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_gravity="center_horizontal" android:text="@string/game_complete_level_complete" android:textStyle="bold" android:textSize="24dip" android:typeface="normal" android:layout_margin="4dip"></TextView><LinearLayout android:layout_height="wrap_content" android:orientation="horizontal" android:id="@+id/RelativeLayout01" android:layout_width="fill_parent">
		<Button android:layout_height="wrap_content" android:id="@+id/game_complete_menu" android:text="@string/game_complete_menu" android:layout_weight="0.5" android:layout_width="wrap_content" android:layout_margin="3dip"></Button>
		<Button android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/game_complete_next" android:text="@string/game_complete_next" android:layout_weight="0.5" android:layout_margin="3dip"></Button>
	
	</LinearLayout>

</LinearLayout>
